{
    "path-intellisense.mappings": {
        "/": "${workspaceFolder}",
        "@": "${workspaceFolder}",
    },
    "typescript.suggest.paths": false,
    "javascript.suggest.paths": false,
    "alias-tool.mappings": {
        "@": "${folder}",
        "@/app": "${folder}/app",
    },
    "files.associations": {
        ".css": "postcss"
    },
    "css.validate": false,
    "[typescriptreact]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[typescript]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    }
}